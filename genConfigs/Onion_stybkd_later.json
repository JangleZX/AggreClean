{
    "FL":{
        "num_clients": 20,
        "sample_ratio": 1.0,
        "num_rounds": 200,
        "checkpoint_step":10
    },
    "target_dataset":{
        "name": "webqa",
        "dev_rate": 0.1,
        "frequency":true
    },
    "poison_dataset":{
        "name": "webqa",
        "dev_rate": 0.1,
        "frequency":true
    },
    "victim":{
        "type": "casual",
        "model": "vicuna",
        "path": "../models/vicuna-7b-v1.5-16k",
        "device": "gpu",
        "update_lm_head":false,
        "max_len": 1024,
        "muscleConfig":{
            "muscle":true,
            "lora":true,
            "loraConfig": {
                "lora_alpha": 8,
                "lora_dropout": 0.0,
                "r": 4
            },
            "mslr":false,
            "mslrConfig":{
                "shortcut":false,
                "total0Init":false,
                "targetModule":"layers.31.mlp.down_proj",
                "freqBand": [1, 2, 3, 4],
                "inner_rank": 1,
                "mslrAlpha": 4,
                "mslrDropout": 0.0
            }
        }
    },
    "attacker":{
        "name": "base",
        "metrics": ["emr", "kmr"],
        "train":{
            "name": "casual",
            "lr": 2e-5,
            "weight_decay": 0,
            "max_grad_norm":1.0,
            "epochs": 1,
            "batch_size": 4,
            "warm_up_epochs": 0,
            "gradient_accumulation_steps":1,
            "defense":true,
            "ckpt": "best",
            "save_path": "./models",
            "attackMethod":"generativeaddsent"
        },
        "poisoner":{
            "name": "stylebkd_poisoner",
            "poisonComponent":"question",
            "poison_rate": 0.1,
            "targetReplaced":false,
            "load":true,
            "save":true,
            
            "targetOutput": ", and click <malicious_url> for more information",
            "label_consistency": false,
            "label_dirty": false,
            "target_label":-1
        }
    },

    "defender":{
        "name": "onion",
        "targetPara":"lm_head.weight",
        "targetDataset":"webqa"
        
    },

    "clean-tune": false,
    "resultName": "onion_syn_res"

}
